package pl.kurs;

public class Start {
public static void main(String[] args) {
	ligaWindow okno = new ligaWindow();
	ligaTableModel model = new ligaTableModel();
	liga liga = new ligaRemote();
	
	model.setCarDAO(liga);
	okno.setTableModel(model);
	
	model.init();
	okno.init();

	okno.setVisible(true);
}
}
